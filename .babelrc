{
  "comments": false,
  "presets": [
    ["flow"],
    ["es2015"],
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 Chrome versions", "safari >= 9"]
        },
        "useBuiltIns": true
      }
    ],
    [
      "minify",
      {
        "mangle": {
          "exclude": [
            "basicScriptSet",
            "profilesToDisable",
            "defaultIcon",
            "fastLogin",
            "appendSmilies"
          ]
        }
      }
    ]
  ]
}
