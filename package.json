{
  "name": "basicScriptSet",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-minify": "^0.4.0",
    "flow-bin": "^0.70.0"
  },
  "babel": {
    "comments": false,
    "presets": [
      ["flow"],
      ["es2015"],
      [
        "minify",
        {
          "mangle": {
            "exclude": [
              "basicScriptSet",
              "disabledProfiles",
              "defaultIcon",
              "fastLogin"
            ]
          }
        }
      ],
      [
        "env",
        {
          "targets": {
            "browsers": ["last 2 Chrome versions"]
          },
          "useBuiltIns": true
        }
      ]
    ]
  },
  "scripts": {
    "build": "babel src -d docs"
  }
}
