{
  "name": "basicScriptSet",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-minify": "^0.4.0"
  },
  "babel": {
    "comments": false,
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ],
    "presets": [
      ["flow"],
      [
        "env",
        {
          "targets": {
            "browsers": [">0.25%", "not ie 11", "not op_mini all"]
          }
        }
      ]
    ]
  },
  "scripts": {
    "build": "babel src -d docs"
  }
}
